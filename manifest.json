{
    "manifest_version": 3,
    "name": "Raeting",
    "description": "cbt for ML",
    "version": "1.1",
    "browser_action": {
        "default_icon": "images/raeting.png",
        "default_popup": "popup.html"
    },

      "permissions": [
        "*://*/*",
        "storage",
        "tabs",
        "cookies",
        "management",
        "contextMenus",
        "activeTab",
        "https://ajax.googleapis.com/"
    ],
    "content_scripts": [{
        "matches": ["*://*/*"],
        "all_frames": true,
        "js": ["js/content.js"]
    }]
}
